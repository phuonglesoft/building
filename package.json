{
  "name": "building-management",
  "version": "1.0.0",
  "description": "Building Management System API with TypeScript and MySQL using Knex.js",
  "main": "dist/src/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "build:watch": "tsc --watch",
    "migrate": "knex migrate:latest",
    "migrate:latest": "knex migrate:latest --knexfile knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile knexfile.ts",
    "migrate:make": "knex migrate:make --knexfile knexfile.ts --extension ts",
    "seed": "knex seed:run",
    "seed:make": "knex seed:make",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "building-management",
    "typescript",
    "nodejs",
    "express",
    "mysql",
    "knex"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/knex": "^0.16.1",
    "aws-jwt-verify": "^5.1.0",
    "aws-sdk": "^2.1692.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "mysql2": "^3.14.3",
    "objection": "^3.1.5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.19.9",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
